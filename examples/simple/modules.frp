%furipota/1
import core "prelude"

define my-module =
  -message: "Hello"
  -target: "world"
  -heh: "nope"

define no-modules =
  Terminal.display-line "{my-module.message}, {my-module.target}"

export define main =
  open my-module excluding heh
  in open Terminal exposing display-line as show
  in 
    show "{message}, {target}"